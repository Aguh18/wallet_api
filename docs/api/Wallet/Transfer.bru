meta {
  name: "Transfer"
  type: http
  seq: 12
}

post {
  url: {{base_url}}/v1/wallets/:id/transfer
  body: json
  auth: none
}

headers {
  Content-Type: application/json
  Cookie: access_token={{access_token}}; refresh_token={{refresh_token}}
}

params:path {
  id: {{wallet_id}}
}

body:json {
  {
    "to_wallet_id": "{{to_wallet_id}}",
    "amount": "25000.75",
    "description": "Transfer untuk jajan"
  }
}
