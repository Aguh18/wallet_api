meta {
  name: "Login"
  type: http
  seq: 2
}

post {
  url: {{base_url}}/v1/auth/login
  body: json
  auth: none
}

body:json {
  {
    "username": "testuser",
    "password": "password123"
  }
}

script:res {
  const accessCookie = res.getResponseCookie("access_token");
  const refreshCookie = res.getResponseCookie("refresh_token");
  bru.setVar("access_token", accessCookie.value);
  bru.setVar("refresh_token", refreshCookie.value);
}
